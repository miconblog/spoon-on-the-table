{
  "root": true,
  "env":{
    "node": true,
    "browser": true,
    "es6": true,
    "jest/globals": true
  },
  "globals": {
    "google": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true,
      "experimentalObjectRestSpread": true
    }
  },
  "plugins": [
    "react",
    "babel",
    "jest",
    "prefer-object-spread"
  ],
  "extends": [
    "airbnb"
  ],
  "rules": {
    "react/prop-types": 0,
    "react/jsx-one-expression-per-line": 0,
    "object-curly-newline":"off"
  },
  "settings": {
    "react": {
      "pragma": "React",
      "version": "16.4.0"
    }
  }
}