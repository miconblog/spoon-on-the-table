{
  "root": true,
  "env":{
    "node": true,
    "browser": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true,
      "experimentalObjectRestSpread": true
    }
  },
  "plugins": [
    "react",
    "class-property",
    "prefer-object-spread"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "rules": {
    "comma-dangle": 0,
    "react/prop-types": 0,
    "react/react-in-jsx-scope": 0,
    "react/jsx-uses-vars": 1,
    "react/display-name": 1,
    "no-unused-vars": 0,
    "no-console": 0,
    "no-unexpected-multiline": "warn",
    "class-property": 0,
    "prefer-object-spread/prefer-object-spread": 2,
    "semi": 1
  },
  "settings": {
    "react": {
      "pragma": "React",
      "version": "16.2.0"
    }
  }
}